{
  "name": "rncpp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --verbose",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "djini:android": "./run_djinni.sh && cd android && ./gradlew clean && cd .. && yarn android",
    "djini:ios": "./run_djinni.sh && yarn ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-mdpi/ && rm -rf ./android/app/src/main/res/raw/",
    "release-android": "yarn build-android && cd ./android && ./gradlew bundleRelease",
    "postinstall": "node ./android/fixAndroid/android-release-fix.js"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.1.0",
    "@expo/vector-icons": "^10.2.1",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@sentry/react-native": "^1.6.2",
    "@tensorflow-models/blazeface": "^0.0.5",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow-models/posenet": "^2.2.1",
    "@tensorflow/tfjs": "^2.3.0",
    "@tensorflow/tfjs-react-native": "^0.3.0",
    "color": "^3.1.2",
    "expo-asset": "^8.1.7",
    "expo-asset-utils": "^1.2.0",
    "expo-camera": "^8.2.0",
    "expo-font": "^8.2.2",
    "expo-gl": "^8.2.0",
    "expo-gl-cpp": "^8.2.0",
    "expo-image-manipulator": "^8.1.0",
    "expo-splash-screen": "^0.5.0",
    "jasmine-core": "^3.5.0",
    "jpeg-js": "^0.4.0",
    "lodash": "^4.17.19",
    "pod-install": "^0.1.7",
    "postinstall": "^0.7.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-animatable": "^1.3.3",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-picker": "^2.3.1",
    "react-native-magnus": "1.0.40",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1",
    "react-native-svg": "^12.1.0",
    "react-native-unimodules": "^0.11.0",
    "react-native-vector-icons": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/react": "^16.9.38",
    "babel-jest": "^26.0.1",
    "djinni": "dropbox/djinni",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
